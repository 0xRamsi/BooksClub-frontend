		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"clean-webpack-plugin": "^4.0.0",
		"eslint": "^8.41.0",
		"eslint-plugin-react": "^7.32.2",
		"html-webpack-plugin": "^5.5.1",
		"prettier": "^2.8.8",
		"ts-loader": "^9.4.3",
		"typescript": "^5.0.4",
